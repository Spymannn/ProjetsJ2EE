<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>action</servlet-name>
        <servlet-class>org.apache.struts.action.ActionServlet</servlet-class>
        <init-param>
            <param-name>config</param-name>
            <param-value>/WEB-INF/struts-config.xml</param-value>
        </init-param>
        <init-param>
            <param-name>debug</param-name>
            <param-value>2</param-value>
        </init-param>
        <init-param>
            <param-name>detail</param-name>
            <param-value>2</param-value>
        </init-param>
        <load-on-startup>2</load-on-startup>
    </servlet>
    <servlet>
        <servlet-name>Connexion</servlet-name>
        <servlet-class>servlets.Connexion</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>initialisationConnexion</servlet-name>
        <servlet-class>servlets.initialisationConnexion</servlet-class>
        <load-on-startup>1</load-on-startup>
    </servlet>
    <servlet>
        <servlet-name>Deconnexion</servlet-name>
        <servlet-class>servlets.Deconnexion</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>rechercheChambre</servlet-name>
        <servlet-class>servlets.rechercheChambre</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>inscriptionMembre</servlet-name>
        <servlet-class>servlets.inscriptionMembre</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Reservation</servlet-name>
        <servlet-class>servlets.Reservation</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CreationReservation</servlet-name>
        <servlet-class>servlets.CreationReservation</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ConfirmationReserv</servlet-name>
        <servlet-class>servlets.ConfirmationReserv</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>suppressionReservation</servlet-name>
        <servlet-class>servlets.suppressionReservation</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ConfirmationSupp</servlet-name>
        <servlet-class>servlets.ConfirmationSupp</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>action</servlet-name>
        <url-pattern>*.do</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Connexion</servlet-name>
        <url-pattern>/Connexion</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>initialisationConnexion</servlet-name>
        <url-pattern>/initialisationConnexion</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Deconnexion</servlet-name>
        <url-pattern>/Deconnexion</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>rechercheChambre</servlet-name>
        <url-pattern>/rechercheChambre</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>inscriptionMembre</servlet-name>
        <url-pattern>/inscriptionMembre</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Reservation</servlet-name>
        <url-pattern>/Reservation</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CreationReservation</servlet-name>
        <url-pattern>/CreationReservation</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ConfirmationReserv</servlet-name>
        <url-pattern>/ConfirmationReserv</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>suppressionReservation</servlet-name>
        <url-pattern>/suppressionReservation</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ConfirmationSupp</servlet-name>
        <url-pattern>/ConfirmationSupp</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>index.jsp</welcome-file>
    </welcome-file-list>
    <jsp-config>
        <taglib>
            <taglib-uri>/WEB-INF/struts-bean.tld</taglib-uri>
            <taglib-location>/WEB-INF/struts-bean.tld</taglib-location>
        </taglib>
        <taglib>
            <taglib-uri>/WEB-INF/struts-html.tld</taglib-uri>
            <taglib-location>/WEB-INF/struts-html.tld</taglib-location>
        </taglib>
        <taglib>
            <taglib-uri>/WEB-INF/struts-logic.tld</taglib-uri>
            <taglib-location>/WEB-INF/struts-logic.tld</taglib-location>
        </taglib>
        <taglib>
            <taglib-uri>/WEB-INF/struts-nested.tld</taglib-uri>
            <taglib-location>/WEB-INF/struts-nested.tld</taglib-location>
        </taglib>
        <taglib>
            <taglib-uri>/WEB-INF/struts-tiles.tld</taglib-uri>
            <taglib-location>/WEB-INF/struts-tiles.tld</taglib-location>
        </taglib>
    </jsp-config>
</web-app>
